{
  "name": "oca-proxy",
  "version": "1.0.9",
  "description": "OpenAI-compatible proxy for Oracle Code Assist (OCA)",
  "main": "dist/index.js",
  "bin": {
    "oca-proxy": "bin/oca-proxy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banzalik/oca-proxy.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": "bin/oca-proxy.js",
    "dev": "tsx src/index.ts",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "1.13.2",
    "express": "5.2.1",
    "jwt-decode": "4.0.0",
    "open": "11.0.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/express": "5.0.6",
    "@types/node": "24.0.0",
    "@types/uuid": "11.0.0",
    "esbuild": "0.27.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "husky": "9.1.7"
  },
  "engines": {
    "node": ">=24.0.0 <25"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/**",
    "dist/**",
    "README.md",
    "LICENSE"
  ]
}
